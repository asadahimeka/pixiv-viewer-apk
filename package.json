{
  "name": "pixiv-viewer",
  "version": "1.25.5",
  "private": true,
  "scripts": {
    "serve": "cross-env VUE_APP_PLATFORM=android vue-cli-service serve --port 8080",
    "build:and:dev": "cross-env BROWSERSLIST_ENV=development VUE_APP_PLATFORM=android npm run build:web -- --modern && npm run cap:sync:and",
    "build:ios:dev": "cross-env BROWSERSLIST_ENV=development VUE_APP_PLATFORM=ios npm run build:web -- --modern && npm run cap:sync:ios",
    "build:and": "cross-env BROWSERSLIST_ENV=capacitor VUE_APP_PLATFORM=android npm run build:web && npm run cap:sync:and",
    "build:ios": "cross-env BROWSERSLIST_ENV=capacitor VUE_APP_PLATFORM=ios npm run build:web && npm run cap:sync:ios",
    "build:web": "vue-cli-service build",
    "cap:sync:and": "npx cap sync android",
    "cap:sync:ios": "npx cap sync ios",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@capacitor-community/file-opener": "1.0.6",
    "@capacitor/android": "5.6.0",
    "@capacitor/app": "5.0.6",
    "@capacitor/clipboard": "5.0.6",
    "@capacitor/core": "5.6.0",
    "@capacitor/device": "5.0.6",
    "@capacitor/ios": "5.6.0",
    "@capacitor/share": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@capgo/capacitor-navigation-bar": "6.0.12",
    "@himeka/capacitor-filesystem": "5.1.2-alpha.2",
    "@himeka/capacitor-plugin-filedownload": "1.0.6",
    "capacitor-android-shortcuts": "5.0.5",
    "capacitor-blob-writer": "1.1.14",
    "capacitor-native-settings": "5.0.1",
    "capacitor-plugin-safe-area": "2.0.6",

    "@vant/touch-emulator": "1.4.0",
    "axios": "0.29.0",
    "better-justified-layout": "1.0.0",
    "core-js": "3.40.0",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.13",
    "gif.js": "0.2.0",
    "jszip": "3.10.1",
    "localforage": "1.10.0",
    "lodash": "4.17.21",
    "mint-filter": "4.0.3",
    "modern-mp4": "0.1.2",
    "nprogress": "0.2.0",
    "swiper": "5.4.5",
    "ts-whammy": "1.1.5",
    "vant": "2.13.2",
    "vue": "2.7.16",
    "vue-awesome-swiper": "4.1.1",
    "vue-i18n": "8.28.2",
    "vue-router": "3.6.5",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@capacitor/assets": "3.0.4",
    "@capacitor/cli": "5.6.0",

    "@rushstack/eslint-patch": "1.7.2",
    "@types/crypto-js": "4.2.2",
    "@types/gif.js": "0.2.5",
    "@types/lodash": "4.17.15",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/eslint-config-standard": "8.0.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.8",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.23.0",
    "postcss-pxtorem": "5.1.1",
    "stylus": "0.64.0",
    "stylus-loader": "7.1.3",
    "xml-loader": "1.2.1"
  }
}
